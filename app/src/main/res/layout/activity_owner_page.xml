<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ownerPage">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Details"
        android:textColor="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.053" />

    <EditText
        android:id="@+id/loc"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:hint="Location"
        android:minHeight="48dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.035" />

    <EditText
        android:id="@+id/roomType"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:hint="Room Type"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loc"
        app:layout_constraintVertical_bias="0.013" />

    <EditText
        android:id="@+id/hospitals"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:hint="Near by Hospitals"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/roomType"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/ps"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:hint="Near by Police Station"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hospitals"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/rs"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:hint="Near by Railway Station"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ps"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/home_image"
        android:layout_width="170dp"
        android:layout_height="200dp"
        android:src="@drawable/image"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rs"
        app:layout_constraintVertical_bias="0.121" />
    <ImageView
        android:id="@+id/check"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/correct"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.871"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rs"
        app:layout_constraintVertical_bias="0.121" />

    <ImageView
        android:id="@+id/dimage"
        android:layout_width="170dp"
        android:layout_height="200dp"
        android:src="@drawable/image"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.871"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rs"
        app:layout_constraintVertical_bias="0.121" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/home_image_btn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="#C2EA94"
        android:text="+ Home Image"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/home_image"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/home_image"
        app:layout_constraintTop_toBottomOf="@+id/home_image"
        app:layout_constraintVertical_bias="0.13" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/d_btn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#C2EA94"
        android:text="+ 3D Room Image"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/dimage"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/dimage"
        app:layout_constraintTop_toBottomOf="@+id/dimage"
        app:layout_constraintVertical_bias="0.186" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/upload"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:background="@drawable/btn_back"
        android:text="Upload"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_image_btn" />


</androidx.constraintlayout.widget.ConstraintLayout>