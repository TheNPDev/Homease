<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Muradhome">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="380dp"
        android:layout_height="350dp"
        android:src="@drawable/murad"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.149" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/threeD"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_back"
        android:text="View in 360"
        android:textAllCaps="false"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.126" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/ar"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_back"
        android:text="AR Navigation (Sample)"
        android:textAllCaps="false"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/threeD"
        app:layout_constraintVertical_bias="0.105" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/vr"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_back"
        android:text="Watch in VR"
        android:textAllCaps="false"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ar"
        app:layout_constraintVertical_bias="0.116" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/book"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:background="@drawable/location_back"
        android:text="Pre-Book Now"
        android:textAllCaps="false"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.574"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ar"
        app:layout_constraintVertical_bias="0.89" />


</androidx.constraintlayout.widget.ConstraintLayout>